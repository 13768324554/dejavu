{
    "name":"dejavu",
    "version":"0.9.1",
    "icons":{
        "16":"site/assets/img/icon_16.png",
        "32":"site/assets/img/icon_32.png",
        "48":"site/assets/img/icon_48.png",
        "128":"site/assets/img/icon_128.png"
    },
    "background": {
        "scripts": [ "background.js" ]
    },
    "content_security_policy": "script-src 'self' https://appbaseio.auth0.com https://cdn.auth0.com blob: filesystem: chrome-extension-resource:",
    "web_accessible_resources": [
        "site/index.html"
    ],
    "permissions": ["storage", "system.display", "<all_urls>", "https://appbaseio.auth0.com/*"],
    "manifest_version": 2,
    "offline_enabled":true,
    "browser_action": {
        "default_icon": "site/assets/img/icon_32.png",
        "default_title": "Dejavu"
    },
    "description": "The missing Web UI for Elasticsearch.",
    "author": "Farhan Chauhan, appbase.io"
}